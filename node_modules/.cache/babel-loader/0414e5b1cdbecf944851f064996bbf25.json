,EAAA,KAAK,EAAC;;;AAER,EAAA,KAAK,EAAC;;;8BACT,mBAAA,CAA+B,KAA/B,EAA+B;AAA1B,EAAA,KAAK,EAAC;AAAoB,CAA/B,EAAmB,QAAnB,EAAyB,CAAA,CAAzB,C;;;AACK,EAAA,KAAK,EAAC;;;AAER,EAAA,KAAK,EAAC;;;;;;;;;;;uBAbnB,mBAAA,CA4BM,KA5BN,EAAA,UAAA,EA4BM,CA3BJ,mBAAA,CA0BM,KA1BN,EAAA,UAAA,EA0BM,CAzBJ,UAyBI,G,kBAxBJ,mBAAA,CAuBM,SAvBN,EAuBM,IAvBN,EAuBM,WAAA,CAvBgC,KAAA,CAAA,KAuBhC,EAvBqC,UAAb,IAAa,EAAT;yBAAlC,mBAAA,CAuBM,KAvBN,EAuBM;AAvBD,MAAA,KAAK,EAAC,WAuBL;AAvBwC,MAAA,GAAG,EAAE,IAAI,CAAC;AAuBlD,KAvBN,E,CACE,mBAAA,CAqBM,KArBN,EAAA,UAAA,EAqBM,CApBJ,mBAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFJ,UAEI,EADJ,mBAAA,CAAiE,KAAjE,EAAA,UAAA,EAAiE,gBAAA,CAA3C,IAAI,CAAC,SAAsC,CAAA,GAA1B,GAA0B,GAAzB,gBAAA,CAAG,IAAI,CAAC,QAAR,CAAxC,EAAwD,CAAxD,CACI,CAHN,CAoBI,EAhBJ,mBAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFJ,UAEI,EADJ,mBAAA,CAAyC,KAAzC,EAAA,WAAA,EAAyC,gBAAA,CAAnB,IAAI,CAAC,KAAc,CAAzC,EAAgC,CAAhC,CACI,CAHN,CAgBI,EAZJ,mBAAA,CAWM,KAXN,EAAA,WAAA,EAWM,CAVO,IAAI,CAAC,S,kBAAhB,mBAAA,CAIM,KAJN,EAIM,WAJN,EAIM,CAHJ,mBAAA,CAES,QAFT,EAES;AAFD,MAAA,KAAK,EAAC,UAEL;AAFiB,MAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,eAAE,QAAA,CAAA,kBAAA,CAAmB,IAAI,CAAC,KAAxB,CAAF;AAAA;AAEtB,KAFT,EAAiE,mBAAjE,EAEA,CAFA,EAEA,WAFA,CAGI,CAJN,C,mBAKA,mBAAA,CAIM,KAJN,EAIM,WAJN,EAIM,CAHJ,mBAAA,CAES,QAFT,EAES;AAFD,MAAA,KAAK,EAAC,QAEL;AAFe,MAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,eAAE,QAAA,CAAA,kBAAA,CAAmB,IAAI,CAAC,KAAxB,CAAF;AAAA;AAEpB,KAFT,EAA+D,iBAA/D,EAEA,CAFA,EAEA,WAFA,CAGI,CAJN,C,CAKI,CAXN,CAYI,CArBN,C,CADF,C;GAuBM,CAvBN,E,GAAA,CAwBI,EA1BN,CA2BI,CA5BN,C","sourcesContent":["<template>\n  <div class=\"parent\">\n    <div class=\"users\">\n      <h2 class=\"usersHeader\">Users</h2>\n      <div class=\"usersList\" v-for=\"user in users\" :key=\"user.id\">\n        <div class=\"userItem\">\n          <div class=\"labele\">\n            <div class=\"label\">Name:</div>\n            <div class=\"label\">{{ user.firstName }} {{ user.lastName }}</div>\n          </div>\n          <div class=\"labele\">\n            <div class=\"label\">Email:</div>\n            <div class=\"label\">{{ user.email }}</div>\n          </div>\n          <div class=\"labele\">\n            <div v-if=\"user.is_active\">\n              <button class=\"deactBtn\" @click=\"changeUserActivity(user.email)\">\n                Deactivate user\n              </button>\n            </div>\n            <div v-else>\n              <button class=\"actBtn\" @click=\"changeUserActivity(user.email)\">\n                Activate user\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"Users\",\n  data() {\n    return {\n      users: [],\n    };\n  },\n  async mounted() {\n    const response = await fetch(\"http://127.0.0.1:8000/api/users\", {\n      method: \"GET\",\n      headers: { \"Content-Type\": \"application/json\" },\n      credentials: \"include\",\n    });\n    var content = await response.json();\n    console.log(content);\n    this.users = content;\n  },\n  methods: {\n    async changeUserActivity(email) {\n      console.log(email);\n      // const idString = id.toString();\n      // console.log(idString)\n      const response = await fetch(\"http://127.0.0.1:8000/api/user/activity\", {\n        method: \"PUT\",\n        headers: { \"Content-Type\": \"application/json\" },\n        credentials: \"include\",\n        body: JSON.stringify({\n          email,\n        }),\n      });\n      let content = await response.json();\n      console.log(\"content \", content);\n      if (response.status == 200) {\n        for (let i in this.users) {\n          if (this.users[i].email == email) {\n            this.users[i].is_active = !this.users[i].is_active;\n          }\n        }\n      }\n    },\n  },\n};\n</script>\n\n<style>\n.parent {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n}\n\n.users {\n  width: 60%;\n  height: 100%;\n  display: flex;\n  padding-bottom: 25px;\n  margin: 30px;\n  flex-direction: column;\n  align-items: center;\n  color: white;\n  background-color: rgb(8, 8, 41);\n  border-radius: 30px;\n\n}\n\n/* .usersList { */\n/* border-radius: 20px; */\n/* box-sizing: border-box; */\n/* height: 630px; */\n/* padding: 20px; */\n/* width: 380px; */\n/* } */\n\n.usersList {\n  width: 100%;\n  display: flex;\n  justify-content: center;\n}\n\n.userItem {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-around;\n  align-items: center;\n  background-color: #303245;\n  border-radius: 12px;\n  border: 0;\n  box-sizing: border-box;\n  color: #eee;\n  font-size: 16px;\n  height: 80%;\n  outline: 0;\n  padding: 4px 20px 0;\n  margin: 7px;\n  width: 90%;\n}\n\n.labele {\n  display: flex;\n  flex-direction: row;\n  margin: 10px;\n  width: 33%;\n  align-items: center;\n  justify-content: center;\n}\n\n.label {\n  margin: 3px;\n}\n\n.actBtn {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  background-color: #07cc69;\n  border: none;\n  color: white;\n  padding: 6px 16px;\n  text-align: center;\n  text-decoration: none;\n  display: inline-block;\n  font-size: 14px;\n  box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);\n  text-transform: uppercase;\n  width: 100%;\n  border-radius: 20px;\n}\n\n.deactBtn {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  background-color: #ff3838;\n  border: none;\n  color: white;\n  padding: 6px 16px;\n  text-align: center;\n  text-decoration: none;\n  display: inline-block;\n  font-size: 14px;\n  box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);\n  text-transform: uppercase;\n  width: 100%;\n  border-radius: 20px;\n}\n\n.input {\n  background-color: #303245;\n  border-radius: 12px;\n  border: 0;\n  box-sizing: border-box;\n  color: #eee;\n  font-size: 16px;\n  height: 100%;\n  outline: 0;\n  padding: 4px 20px 0;\n  width: 100%;\n}\n</style>"],"sourceRoot":""}]}