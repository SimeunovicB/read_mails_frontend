AAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;AACd","file":"/Users/mac/PycharmProjects/read_mails_frontend/src/components/LeadsTypeAndSearch.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"typesAndSearch\">\n    <div class=\"types\">\n      <button\n        class=\"btn\"\n        v-bind:style=\"{\n          'background-color': activeBtn == 'TARGET LEADS' ? '#11bfeb' : 'white',\n          'color': activeBtn == 'TARGET LEADS' ? 'white' : 'black',\n        }\"\n        @click=\"buttonOne()\"\n      >\n        Target Leads\n      </button>\n      <button\n        class=\"btn\"\n        v-bind:style=\"{\n          'background-color': activeBtn == 'ALL LEADS' ? '#11bfeb' : 'white',\n          'color': activeBtn == 'ALL LEADS' ? 'white' : 'black',\n        }\"\n        @click=\"buttonTwo()\"\n      >\n        All leads\n      </button>\n      <button\n        class=\"btn\"\n        v-bind:style=\"{\n          'background-color': activeBtn == 'MY LEADS' ? '#11bfeb' : 'white',\n          'color': activeBtn == 'MY LEADS' ? 'white' : 'black',\n        }\"\n        @click=\"buttonThree()\"\n      >\n        My leads\n      </button>\n      <button\n        class=\"btn\"\n        v-bind:style=\"{\n          'background-color': activeBtn == 'SORT BY SHIPPING DATE' ? '#11bfeb' : 'white',\n          'color': activeBtn == 'SORT BY SHIPPING DATE' ? 'white' : 'black',\n        }\"\n        @click=\"buttonFour()\"\n      >\n        Sort by shipping date\n      </button>\n      <button\n        class=\"btn\"\n        v-bind:style=\"{\n          'background-color': activeBtn == 'UNCLAIMED' ? '#11bfeb' : 'white',\n          'color': activeBtn == 'UNCLAIMED' ? 'white' : 'black',\n        }\"\n        @click=\"buttonFive()\"\n      >\n        Unclaimed\n      </button>\n    </div>\n    <div class=\"search\">\n      <label>Search: </label><input id=\"searchInput\" type=\"text\" />\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"LeadsTypeAndSearch\",\n  data() {\n    return {\n      activeBtn: \"NONE\",\n    };\n  },\n\n  methods: {\n    buttonOne() {\n      this.activeBtn = \"TARGET LEADS\";\n    },\n    buttonTwo() {\n      this.activeBtn = \"ALL LEADS\";\n    },\n    buttonThree() {\n      this.activeBtn = \"MY LEADS\";\n    },\n    buttonFour() {\n      this.activeBtn = \"SORT BY SHIPPING DATE\";\n    },\n    buttonFive() {\n      this.activeBtn = \"UNCLAIMED\"\n    },\n  },\n};\n</script>\n\n<style>\n.btn {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  background-color: white;\n  /* background-color: white; */\n  border: none;\n  color: black;\n  padding: 12px 16px;\n  text-align: center;\n  text-decoration: none;\n  display: inline-block;\n  font-size: 14px;\n  box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);\n  text-transform: uppercase;\n  margin: 0px 5px;\n}\n\n.hover:hover {\n  background-color: green;\n}\n\n.btn:hover {\n  /* background-color: rgb(212, 212, 212); */\n}\n\n.typesAndSearch {\n  text-align: center;\n  display: block;\n  margin: 4% 0% 10% 0%;\n  position: relative;\n}\n\n.types {\n  float: left;\n}\n\n.search {\n  float: right;\n}\n\n#searchInput {\n  width: 60%;\n  height: 20px;\n}\n</style>"]}]}